{
  "name": "crowdstrike-exercise",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": ["babel-jest", {"rootMode": "upward"}]
